{
  "name": "react-component-template",
  "version": "1.0.0",
  "description": "React independent component template",
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest --watch --verbose",
    "start": "webpack-dev-server --progress --colors",
    "build": "rm -rf lib && webpack --config webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanoop-jose/react-component-template.git"
  },
  "keywords": [
    "react",
    "react-component",
    "component",
    "boilerplate",
    "template",
    "npm",
    "package",
    "npm-publish"
  ],
  "author": "Sanoop Jose <snupjos@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanoop-jose/react-component-template/issues"
  },
  "homepage": "https://github.com/sanoop-jose/react-component-template#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.0",
    "enzyme": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.5.2",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "regenerator-runtime": "^0.11.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.4.5"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/mocks",
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
